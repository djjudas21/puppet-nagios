##################################
define <%= @title %> {
		title=<%= @displayname %>
		desc=<%= @desc %>
		primary=<%= @primary %>
		info=<%= @info %>
		members=<% @members.each do |item| -%><%= "#{item['host']}" -%><% if item['service'] %>;<%= "#{item['service']}" -%><% end %><% if item['opt'] %>;<%= "#{item['opt']}" -%><% end %>, <% end %>
		warning_threshold=<%= @warning_threshold %>
		critical_threshold=<%= @critical_threshold %>
		priority=<%= @priority %>
}

