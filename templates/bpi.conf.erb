##################################
define <%= @title %> {
		title=<%= @displayname %>
		desc=<%= @desc %>
		primary=<%= @primary %>
		info=<%= @info %>
		members=<% @members.each_pair do |key, value| -%><%= key -%><%if value['service'] %>;<%= "#{value['service']}" -%><% end %><% if value['opt'] %>;<%= "#{value['opt']}" -%><% end %>, <% end %>
		warning_threshold=<%= @warning_threshold %>
		critical_threshold=<%= @critical_threshold %>
		priority=<%= @priority %>
}

